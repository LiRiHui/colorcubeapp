.page{
  width: 100%;
  height: 100%;
}

page{
  height: 100%;
}

.title-panel {
  background-repeat: no-repeat;
  background-size: 100% 50%;
  background-image: linear-gradient(to bottom right, #ff7307, #ff4f06);
  height: 163rpx;
  display: flex;
  align-items: center;
}

.title-panel image {
  margin-left: 25rpx;
  margin-right: 25rpx;
  height: 135rpx;
  width: 135rpx;
}

.title-panel text {
  line-height: 67rpx;
  height: 135rpx;
  color: white;
}